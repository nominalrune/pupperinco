{
	"compilerOptions": {
		"target": "es6",
		"lib": [
			"dom",
			"esnext",
			"WebWorker"
		],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": false,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"jsx": "preserve",
		"moduleResolution": "node",
		"allowUmdGlobalAccess": true,
		"declaration": true,
		"strictNullChecks": true,
		"noUncheckedIndexedAccess": true,
		"resolveJsonModule": true,
		"isolatedModules": true
	},
	"include": [
		"**/*.d.ts",
		"**/*.ts",
		"**/*.tsx"
	],
	"exclude": [
		"/node_modules"
	]
}